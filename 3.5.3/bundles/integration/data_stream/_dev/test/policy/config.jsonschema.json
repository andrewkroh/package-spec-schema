{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "data_stream": {
      "description": "Configuration for the data stream.",
      "type": [
        "null",
        "object"
      ],
      "properties": {
        "vars": {
          "description": "Variables used to configure settings defined in the data stream manifest.",
          "$ref": "#/$defs/https:~1~1schemas.elastic.dev~1package-spec~13.5.3~1integration~1data_stream~1_dev~1test~1config.jsonschema.json/definitions/vars"
        }
      },
      "additionalProperties": false
    },
    "input": {
      "description": "The input of the package to test.",
      "type": "string"
    },
    "skip": {
      "$ref": "#/$defs/https:~1~1schemas.elastic.dev~1package-spec~13.5.3~1integration~1data_stream~1_dev~1test~1config.jsonschema.json/definitions/skip"
    },
    "vars": {
      "description": "Variables used to configure settings defined in the package manifest.",
      "$ref": "#/$defs/https:~1~1schemas.elastic.dev~1package-spec~13.5.3~1integration~1data_stream~1_dev~1test~1config.jsonschema.json/definitions/vars"
    }
  },
  "additionalProperties": false,
  "$defs": {
    "https://schemas.elastic.dev/package-spec/3.5.3/integration/data_stream/_dev/test/config.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "type": "object",
      "additionalProperties": false,
      "definitions": {
        "skip": {
          "description": "If this test should be skipped, more information about why it was skipped.",
          "type": "object",
          "required": [
            "reason",
            "link"
          ],
          "properties": {
            "link": {
              "description": "Link to issue with more details about skipped test or to track re-enabling skipped test.",
              "type": "string",
              "example": "https://github.com/elastic/integrations/issues/520"
            },
            "reason": {
              "description": "Short explanation for why test has been skipped.",
              "type": "string",
              "example": "Flaky test"
            }
          },
          "additionalProperties": false
        },
        "vars": {
          "description": "Variables defined for the test case.",
          "type": [
            "null",
            "object"
          ],
          "properties": {
            "data_stream.dataset": {
              "type": "string",
              "pattern": "^[a-zA-Z0-9]+[a-zA-Z0-9\\._]*$"
            }
          }
        }
      }
    }
  }
}
